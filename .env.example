# Base URL for Strapi API (must include /api at the end)
# Example: https://huayca.crub.uncoma.edu.ar/strapi/api
VITE_STRAPI_API_URL=

# Optional: Token for Strapi if content requires auth
VITE_STRAPI_TOKEN=

# Optional: Override base URL used to compose media file URLs from Strapi
# If not set, it will be derived from VITE_STRAPI_API_URL (dropping trailing /api)
VITE_STRAPI_FILES_BASE_URL=
# Public env vars for Vite must start with VITE_
VITE_STRAPI_API_URL=
# Optional: token if Strapi requires auth to read content
VITE_STRAPI_TOKEN=
# Optional: base for Strapi file URLs when not under API base
VITE_STRAPI_FILES_BASE_URL=
